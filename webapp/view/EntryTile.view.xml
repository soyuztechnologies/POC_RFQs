<mvc:View xmlns:core="sap.ui.core"
    controllerName="soyz.dev.poc.pocproject.controller.EntryTile"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns="sap.m"
    xmlns:form="sap.ui.layout.form"
>
<!-- <Shell > -->
    <App id="appEntry">
        <pages>
            <Page>
                <headerContent>
                    <Button icon="sap-icon://log" text="Logout" tooltip="Logout" press="onLogout" type="Emphasized" />
                </headerContent>
                <content>
                    <GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Quotation Requests" subheader="New Requests" press="onTilePress" >
                        <TileContent >
                            <NumericContent value="{local>/ReqForSupplier/length}" icon="sap-icon://request" withMargin="false" />
                        </TileContent>
                    </GenericTile>
                </content>
            </Page>
        </pages>
    </App>
<!-- </Shell> -->
    
</mvc:View>
